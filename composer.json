{
    "name": "wenxin/qrcode",
    "description": "This is a qr code generator that can customize your our QRcode.",
    "keywords": ["qrcode", "qrcode generator", "generator", "frame", "icon"],
    "license" : "MIT",
    "authors": [
        {
            "name": "wenxin",
            "email": "gwenxin0909@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
      	"ext-gd": "*",
        "bacon/bacon-qr-code": "1.0.*",
        "intervention/image": "^2.4",
        "illuminate/support": ">=5.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-4": {
            "Wenxin\\Qrcode\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenxin\\Qrcode\\QrCodeServiceProvider"
            ],
            "aliases": {
                "QrCode": "Wenxin\\Qrcode\\Facades\\QrCode"
            }
        }
    }
}
